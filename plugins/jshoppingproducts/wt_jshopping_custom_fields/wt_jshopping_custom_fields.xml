<?xml version="1.0"?>
<extension type="plugin" method="upgrade" group="jshoppingproducts">
	<name>JoomShoppingProducts - Wt Jshopping custom fields</name>
	<author>Sergey Tolkachyov</author>
	<authorEmail>info@web-tolk.ru</authorEmail>
	<authorUrl>https://web-tolk.ru/</authorUrl>
	<creationDate>April 2024</creationDate>
	<copyright>Sergey Tolkachyov</copyright>
	<license>GNU General Public License version 3 or later</license>
	<version>1.1.0</version>
	<description>JoomShoppingProducts - Wt Jshopping custom fields</description>
	<scriptfile>script.php</scriptfile>
	<namespace path="src">Joomla\Plugin\Jshoppingproducts\Wt_jshopping_custom_fields</namespace>
	<files>
		<folder plugin="wt_jshopping_custom_fields">src</folder>
		<folder>services</folder>
	</files>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field addfieldprefix="Joomla\Plugin\Jshoppingproducts\Wt_jshopping_custom_fields\Fields"
					   type="plugininfo" name="plugininfo"/>
			</fieldset>
		</fields>
	</config>
</extension>